// See https://go.microsoft.com/fwlink/?LinkId=733558
// for the documentation about the tasks.json format
{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "npm",
			"script": "compile",
			"problemMatcher": "$tsc",
			"presentation": {
				"reveal": "silent"
			},
			"options": {
				"shell": {
					"executable": "cmd.exe",
					"args": ["/d", "/c"]
				}
			},
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}
